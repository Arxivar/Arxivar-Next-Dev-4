@using Microsoft.AspNetCore.Authentication

<h2>Claims</h2>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    
    @{
        var list = (await Context.AuthenticateAsync()).Properties.Items;
        foreach (var prop in list)
        {
            <dt>@prop.Key</dt>
            <dd>@prop.Value</dd>
        }
    }
</dl>
